to approve or reject :- 

curl --location --request PUT 'localhost:4000/tutor/approveOrReject' \
--header 'Content-Type: application/json' \
--data '{
    "tutorId":"671dc798e8e5b726e1be0ee9",
    "subject":"Mathematics",
    "approved":true
}'

